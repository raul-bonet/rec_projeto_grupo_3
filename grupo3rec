let grupo=["NICOLAS VIEIRA","RAUL BONET","RENATO DE LIMA",
           "SAMUEL DIAS","WILLYAN GABRIEL",
           "GABRIELLY FERREIRA",
           "LAYO KAUE","MATEUS ANTUNES"];
